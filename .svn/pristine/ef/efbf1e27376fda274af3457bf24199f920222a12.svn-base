@import "../../../../styles-master"

.container
  width: 1168px !important
  height: 701px !important

.frame
  padding-top: 150px
  .slidee
    margin: 0
    padding: 0
    list-style: none
    li
      float: left
      margin: 0 80px 0 0
      padding: 0
      width: 170px
      height: 300px
      transform: scale(1)
      transition: all .2s ease-in-out
      opacity: .6
      @extend .gray-scale
    li.active
      opacity: 1 !important
      transform: scale(1.5)
      @extend .non-gray-scale

.only-kk
  margin-top: 13%
  transform: scale(1.5)

.label-no
  font-size: 20px

.input-no
  font-size: 30px
  height: 80px


#div-login
  top: -21px
  right: 90px
  position: absolute
  display: flex
  .key
    position: absolute
    margin-left: 10%
    transform:  translate(65%, 30px)
  label
    text-align: center
    position: absolute
    transform:  translate(0%, 120px)
    width: 100%
    padding-left: 25px
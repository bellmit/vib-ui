img
  max-width: 100%

.row
  width: 100%

  div
    text-align: center

.container
  align-items: center
  height: 100%

.vertical-center
  display: flex
  vertical-align: middle
  align-items: center


.moveLeftOut
  position: relative
  -webkit-animation-name: moveLeftOut
  -webkit-animation-duration: 2s
  -webkit-animation-iteration-count: 1
  -webkit-animation-fill-mode: forwards

.moveRightOut
  position: relative
  -webkit-animation-name: moveRightOut
  -webkit-animation-duration: 2s
  -webkit-animation-iteration-count: 1
  -webkit-animation-fill-mode: forwards

.flip-cover-h
  -webkit-transform-origin: 0% 100%
  -webkit-animation-name: flip-cover
  -webkit-animation-duration: 1.5s
  -webkit-animation-iteration-count: 1
  -webkit-animation-fill-mode: forwards

@-webkit-keyframes flip-cover
  0%
    transform: rotate3d(1, 0, 0, 170deg)
  100%
    transform: rotate3d(1, 0, 0, 10deg)


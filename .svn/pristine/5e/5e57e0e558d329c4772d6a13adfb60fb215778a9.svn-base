.scroll
  color: black
  border: none
  border-collapse: collapse
  width: 100%
  background-color: white
  margin-top: 2px

  thead
    background-color: #059DA6
    color: white
  tr
    display: flex
  td
    padding: 3px
    flex: 1 auto
    width: 1px
    word-wrap: break-word
    font-style: normal
    font-size: 18px
  thead
    th
      flex: 1 auto
      display: block
      height: 55px
      vertical-align: middle
      text-align: center
      font-size: 18px
      line-height: 15px
      padding-top: 15px
  tbody
    tr.out
      color: #C5251C
      background-color: transparent
    tr.current
      background-color: #009AC7
      color: white
    td
      height: 35px
      vertical-align: middle
      line-height: 30px
  thead th, tbody td
    border-left: 1px solid #392F89
  thead th:first-child
    border-left: none
  tbody
    td:first-child
      border-left: none
    display: block
    width: 100%
    overflow-y: auto
    height: 700px
    background-image: url("/assets/kiatnakin/image/bg-bookbank.png")
    tr:last-child
      height: auto

  th:nth-child(1), td:nth-child(1)
    width: 15%

  th:nth-child(2), td:nth-child(2)
    width: 15%

  th:nth-child(3), td:nth-child(3), th:nth-child(4), td:nth-child(4), th:nth-child(5), td:nth-child(5)
    width: 20%

  th:nth-child(6), td:nth-child(6)
    width: 15%


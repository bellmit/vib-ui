@import "./src/styles-master"

.flexbox-container
    height: 100vh
    display: flex
    justify-content: center
    align-items: center
   
.catalog-container
    height: 518px 
li
    div
        position: relative
        height: 518px
        img 
            position: absolute
            bottom: 0

.div-item-catalog
    display: flex
    align-items: center
    justify-content: center
.item-catalog
    width: 400px !important 
 
.catalog_frame
  width: 100%
  height: 518px
  left: 0
  position: fixed
  margin-top: 0%
  
  .slidee
    margin: 0
    padding: 0
    height: 100%
    list-style: none
    li
      float: left
      margin: 0
      padding: 0
      width: 428px 

.footer-bar
    height: 49px
    width: 100vw
    position: fixed
    background-image: linear-gradient(-180deg, #019CB2 0%, #055F7C 100%)